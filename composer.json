{
    "name": "salsan/clubs",
    "description": "Library to get data about chess clubs from website Federazione Scacchistica Italiana",
    "type": "library",
    "keywords": ["chess", "scacchi", "fsi", "Federazione Scacchistica Italiana"],
    "homepage": "https://github.com/salsan/Clubs",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Salsan\\Clubs\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Salvatore Santagati",
            "email": "github@salsan.dev"
        }
    ],
    "support": {
        "issues": "https://github.com/salsan/Clubs/issues"
    },
    "require": {
        "php": ">=8.1",
        "salsan/utils": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*",
        "squizlabs/php_codesniffer": "3.*",
        "phpbench/phpbench": "^1.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Benchmark\\": "tests/Benchmark"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:salsan/PHPUtils.git"
        }
    ],
    "config":{
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }

}
